    "pushGit": "git push origin main",
    "pullHeroku": "git pull heroku main",
    "pushHeroku": "git push heroku main:main"
  },
  "scripts": {
    "test": "nodemon app.js",
    "start": "node app.js",
    "commit_GIT": "$npm_package_config_commitGit",
    "sync_GIT": "$npm_package_config_pullGit && $npm_package_config_pushGit",
    "sync_HEROKU": "$npm_package_config_pullHeroku && $npm_package_config_pushHeroku"
  },
  "author": "CSE341 Team 2",
  "license": "",
  "devDependencies": {
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "connect-mongodb-session": "^2.0.3",
    "cors": "^2.8.5",
    "csurf": "^1.9.0",
    "dotenv": "^14.3.2",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mongodb": "^3.1.6",
    "mongoose": "^5.2.17",
    "multer": "^1.4.0",
    "mysql2": "^1.6.1",
    "nodemailer": "^4.6.8",
    "nodemailer-sendgrid-transport": "^0.2.0",
